# Copy this file to .env and fill in your Neon (or other PostgreSQL) credentials.
# Spring Boot reads these as environment variables when you run the app.

# JDBC URL for PostgreSQL (e.g. from Neon dashboard)
# Neon format: jdbc:postgresql://HOST/DATABASE?sslmode=require
DATABASE_URL=jdbc:postgresql://localhost:5432/your_database

# Database username
DATABASE_USERNAME=postgres

# Database password
DATABASE_PASSWORD=your_password
